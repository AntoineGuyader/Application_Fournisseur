<Page x:Class="AppFournisseur_WPF.Views.Pages.Page_Accueil"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AppFournisseur_WPF.Views.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page_Accueil"
      Style="{StaticResource Basic_Main_Frame}">


    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="120"/>
        </Grid.RowDefinitions>
        <Label Content="Page d'Accueil" 
                Style="{StaticResource LabelForm}" Grid.Row="0"
                VerticalAlignment="Center" HorizontalAlignment="center"/>

        <ComboBox Name="ComboBoxAccueil"
                  Grid.Row="1" SelectedIndex="0"
                  Width="150" Height="20">
            
        </ComboBox>
    </Grid>
</Page>
